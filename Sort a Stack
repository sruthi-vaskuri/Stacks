class GfG{
	public Stack<Integer> sort(Stack<Integer> s)
	{
		//add code here.
		Stack<Integer> st=new Stack<>();
		while(!s.isEmpty())
		{
		    int temp=s.pop();
		    while(!st.isEmpty() && st.peek()>temp)
		         s.push(st.pop());
		    st.push(temp);
		}
		return st;
	}
}




